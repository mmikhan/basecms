{
  "name": "admarket",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "dev:seed": "payload run src/seed.ts",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation eslint .",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "typecheck": "next typegen && tsc --noEmit",
    "i18n:check": "i18n-check --source en --locales src/i18n/messages"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@payloadcms/admin-bar": "3.58.0",
    "@payloadcms/db-postgres": "3.58.0",
    "@payloadcms/email-resend": "^3.58.0",
    "@payloadcms/live-preview-react": "3.58.0",
    "@payloadcms/next": "3.58.0",
    "@payloadcms/payload-cloud": "3.58.0",
    "@payloadcms/plugin-form-builder": "3.58.0",
    "@payloadcms/plugin-import-export": "3.58.0",
    "@payloadcms/plugin-nested-docs": "3.58.0",
    "@payloadcms/plugin-redirects": "3.58.0",
    "@payloadcms/plugin-search": "3.58.0",
    "@payloadcms/plugin-seo": "3.58.0",
    "@payloadcms/plugin-stripe": "3.58.0",
    "@payloadcms/richtext-lexical": "3.58.0",
    "@payloadcms/translations": "3.58.0",
    "@payloadcms/ui": "3.58.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^10.1.0",
    "dotenv": "17.2.1",
    "embla-carousel-auto-scroll": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "file-type": "^21.0.0",
    "graphql": "^16.11.0",
    "lucide-react": "^0.525.0",
    "next": "15.5.4",
    "next-cloudinary": "^6.16.0",
    "next-intl": "^4.3.9",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "payload": "3.58.0",
    "payload-storage-cloudinary": "^1.1.3",
    "prism-react-renderer": "^2.4.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.63.0",
    "sharp": "0.34.3",
    "states-us": "^1.1.1",
    "stripe": "^18.5.0",
    "tailwind-merge": "^3.3.1",
    "transliteration": "^2.3.5",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@faker-js/faker": "^10.0.0",
    "@lingual/i18n-check": "^0.8.8",
    "@playwright/test": "1.54.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/react": "16.3.0",
    "@types/node": "^24.6.1",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "4.7.0",
    "eslint": "^9.36.0",
    "eslint-config-next": "15.5.2",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ],
    "overrides": {
      "@payloadcms/plugin-cloud-storage": "3.58.0"
    }
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184"
}
